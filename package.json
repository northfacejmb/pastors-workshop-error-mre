{
	"name": "pastors-workshop",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . && ./node_modules/.bin/stylelint ./src/**/*.{scss,svelte} --fix",
		"format": "prettier --plugin-search-dir . --write . && ./node_modules/.bin/stylelint ./src/**/*.{scss,svelte} --fix",
		"getTypes": "storyblok pull-components --space=1016915",
		"setTypes": "storyblok-generate-ts source=./components.1016915.json target=./src/types/storyblok",
		"getComponents": "storyblok pull-components --space=1016915",
		"pushComponents": "storyblok push-components --space=1016915 ./components.1016915.json",
		"types": "storyblok login && npm run getTypes && npm run setTypes",
		"backup": "storyblok login && npm run getTypes && npm run setTypes && node scripts/export_all.js"
	},
	"devDependencies": {
		"@mailchimp/mailchimp_marketing": "3.0.80",
		"@storyblok/svelte": "^4.0.6",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/clone-deep": "^4.0.4",
		"@types/gtag.js": "0.0.12",
		"@types/luxon": "3.3.0",
		"@types/pluralize": "^0.0.33",
		"@typescript-eslint/eslint-plugin": "5.59.2",
		"@typescript-eslint/parser": "5.59.2",
		"eslint": "8.40.0",
		"eslint-config-prettier": "8.8.0",
		"postcss": "^8.4.29",
		"postcss-html": "1.5.0",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"sass": "1.62.1",
		"storyblok-generate-ts": "1.13.2",
		"stylelint": "15.6.1",
		"stylelint-config-html": "1.1.0",
		"stylelint-config-recommended-scss": "11.0.0",
		"stylelint-config-standard": "33.0.0",
		"stylelint-order": "6.0.3",
		"stylelint-scss": "5.0.0",
		"svelte": "4.2.11",
		"svelte-check": "3.4.3",
		"tslib": "2.5.0",
		"typescript": "5.0.4",
		"vite": "^5.0.0",
		"vite-plugin-mkcert": "1.15.0"
	},
	"type": "module",
	"dependencies": {
		"@storyblok/js": "^3.0.8",
		"@vercel/blob": "^0.23.4",
		"bcrypt": "^5.1.1",
		"clone-deep": "^4.0.1",
		"eslint-plugin-svelte": "^2.34.0",
		"instantsearch.js": "^4.64.2",
		"ip-range-check": "^0.2.0",
		"luxon": "3.3.0",
		"mailersend": "^2.1.1",
		"pluralize": "^8.0.0",
		"postcss-scss": "4.0.9",
		"storyblok-js-client": "^6.7.3",
		"stripe": "^14.22.0",
		"svelte-select": "5.6.0",
		"typesense": "^1.7.2",
		"typesense-instantsearch-adapter": "^2.7.1"
	},
	"engines": {
		"npm": ">=9.0.0",
		"node": ">=18.0.0"
	}
}
